        "autoprefixer": "^10.4.2",
        "babel-jest": "^27.5.1",
        "babel-loader": "^8.2.2",
        "core-js": "^3.21.1",
        "cross-env": "^7.0.3",
        "css-loader": "^5.2.6",
        "css-minimizer-webpack-plugin": "^3.0.1",
        "eslint": "^8.9.0",
        "eslint-plugin-react": "^7.28.0",
        "eslint-plugin-react-hooks": "^4.3.0",
        "happypack": "^5.0.1",
        "html-webpack-plugin": "^5.3.2",
        "husky": "^7.0.4",
        "jest": "^27.5.1",
        "lint-staged": ">=10",
        "mini-css-extract-plugin": "^1.6.0",
        "node-sass": "^7.0.1",
